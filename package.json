{
  "name": "function-analytics",
  "version": "1.0.0-beta.1",
  "description": "Produce universal library with webpack and es6",
  "main": "lib/function-analytics.js",
  "scripts": {
    "build": "npm run test && webpack --env dev && webpack --env build --fix && npm run docs",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors test/**/**/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/**/**/*.js",
    "repl": "node -i -e \"$(< ./lib/webpack-library-starter.js)\"",
    "publish": "npm run build && cd lib && npm publish --access public",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hisptz/function-analytics.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Krasimir Tsonev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krasimir/webpack-library-starter/issues"
  },
  "homepage": "https://github.com/krasimir/webpack-library-starter",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-flow-interface-imports": "^1.0.1",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^2.0.1",
    "commitizen": "^3.1.1",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "docdash": "^1.1.0",
    "documentation": "^10.0.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "husky": "^1.3.1",
    "jsdoc": "^3.5.5",
    "jsdom": "15.0.0",
    "jsdom-global": "3.0.2",
    "mocha": "^6.1.2",
    "nyc": "^14.0.0",
    "progress-promise": "0.0.6",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^13.2.2"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
